@article{ ISI:000189157700001,
Author = {Blackmore, J},
Title = {{Tracking the nomadic life of the educational researcher: What future for
   feminist public intellectuals and the performative university?}},
Journal = {{AUSTRALIAN EDUCATIONAL RESEARCHER}},
