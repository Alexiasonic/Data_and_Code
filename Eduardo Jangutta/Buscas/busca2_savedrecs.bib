Language = {{English}},
Affiliation = {{Jimenez-Esquinas, G (Corresponding Author), Spanish Res Council CSIC, Inst Heritage Sci Incipit, Santiago De Compostela, Spain.
   Jimenez-Esquinas, Guadalupe, Spanish Res Council CSIC, Inst Heritage Sci Incipit, Santiago De Compostela, Spain.}},
DOI = {{10.1080/09669582.2016.1206109}},
