@article{ ISI:000225817700001,
Author = {Backstrand, K},
Title = {{Scientisation vs. civic expertise in environmental governance:
   Eco-feminist, eco-modern and post-modern responses}},
Journal = {{ENVIRONMENTAL POLITICS}},
Year = {{2004}},
Volume = {{13}},
Number = {{4}},
Pages = {{695-714}},
Month = {{WIN}},
Note = {{ECPR 2001 Meeting, CANTERBURY, ENGLAND, 2001}},
Organization = {{ECPR}},
Abstract = {{This article examines three critical perspectives in green political
   theory arguing that environmental governance is emerging as an
   increasingly scientised and technocratic domain. These are contrasted
   with work under the banner of `civic expertise' proposing increased
   citizen deliberation and participation in the scientific realm to
   reverse the technocratic features of environmental management.
   Eco-feminism links the rise of technocratic science to an overall
   critique of modernity, rationality and patriarchy. Eco-modemism aims at
   re-configuring scientific rationality in terms of reflexive
   modernisation, and a stronger participatory dimension of civil society.
   In the postmodern green critique, the ascendancy of regulatory science
   marks the influence of biopower or green governmentality. Civic
   expertise is advanced as a middle ground between these contested
   appraisals of science in modem societies. This is underpinned by a
   post-positivist account of scientific knowledge and promotes a reform of
   the scientific endeavour toward enhanced transparency, participation and
   democratisation.}},
Publisher = {{ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD}},
