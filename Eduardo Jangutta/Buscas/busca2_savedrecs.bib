@article{ ISI:000368435200004,
Author = {Ferree, Myra Marx and Zippel, Kathrin},
Title = {{Gender Equality in the Age of Academic Capitalism: Cassandra and
   Pollyanna Interpret University Restructuring}},
Journal = {{SOCIAL POLITICS}},
Year = {{2015}},
