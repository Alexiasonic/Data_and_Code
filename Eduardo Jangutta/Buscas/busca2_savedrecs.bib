@article{ ISI:000459230700002,
Author = {Xiong, Ying and Cho, Moonhee and Boatwright, Brandon},
Title = {{Hashtag activism and message frames among social movement organizations:
   Semantic network analysis and thematic analysis of Twitter during the
   \#MeToo movement}},
Journal = {{PUBLIC RELATIONS REVIEW}},
Year = {{2019}},
Volume = {{45}},
Number = {{1}},
Pages = {{10-23}},
Month = {{MAR}},
Abstract = {{During the \#MeToo movement, social movement organizations (SMOs) played
   a crucial role in the online mobilization by utilizing various message
   frames and appealing hashtags during the social movement. Applying a
   co-creational approach and using framing as a theoretical framework, the
   study explored how SMOs use words and hashtags to participate in the
   \#MeToo movement through Twitter. Based on both semantic network
   analysis and thematic analysis methods, findings of the study enhance
   literature of social movement organizations and activism as well as
   provide practical implications for effective social movement campaigns.}},
