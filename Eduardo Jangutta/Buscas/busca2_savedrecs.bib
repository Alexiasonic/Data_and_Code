@article{ ISI:000462422300003,
Author = {Kamla, Rania},
Title = {{Religion-based resistance strategies, politics of authenticity and
   professional women accountants}},
Journal = {{CRITICAL PERSPECTIVES ON ACCOUNTING}},
Year = {{2019}},
Volume = {{59}},
Pages = {{52-69}},
Month = {{MAR}},
Abstract = {{This study examines resistance strategies to patriarchal interpretations
   of women's roles in Islam by Syrian women accountants (SWAB). It expands
   discussions on gender subjectivity construction and resistance in the
   profession by focusing on women's religion-based strategies in a
   non-western context. The study introduces debates from Muslim Feminism
   on resistance through egalitarian and ethics-based re-interpretations of
   religious texts. It parallels these with ordinary professional Muslim
   women accountants' strategies. The article finds that SWAB significantly
   mobilise religion-based strategies to contest patriarchal
   interpretations of their roles in Islam, and to secure access and
   progress in the profession. These strategies go beyond shifting between
   defensive/compliance and offensive/challenging approaches to incorporate
   a third authentic/ethical dimension often neglected in professional and
   organization research on women's resistance strategies. The paper
   concludes that a greater theoretical and empirical understanding of
   resistance within religion and ethics enhances appreciation of
   distinctive and diverse ways that gendering in professions is resisted
   and transformed. Crown Copyright (C) 2018 Published by Elsevier Ltd. All
   rights reserved.}},
