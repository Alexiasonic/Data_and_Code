@article{ ISI:000469881300007,
Author = {Pegu, Sanjana},
Title = {{MeToo in India: building revolutions from solidarities}},
Journal = {{DECISION}},
Year = {{2019}},
Volume = {{46}},
Number = {{2, SI}},
Pages = {{151-168}},
Month = {{JUN}},
Abstract = {{In this article, I curate diverse perspectives on the MeToo movement in
   India and its subsequent crosscurrents. Drawing on in-depth interviews
   with five women from different backgrounds, I explore the movement's
   significance, impact, unresolved questions, and potential solutions.
   Through our dialogues, we interrogated and unpacked myriad issues,
   including rampant gender-based violence at our workplaces and in our
   homes, the repeated failure of due process, and the need to move towards
   a politics of solidarity and mutual empowerment, rather than one steeped
   in an individualist, capitalist patriarchy. I raise questions for
   academic and activist discussion, deliberation, and enquiry.}},
