@article{ ISI:000356625400004,
Author = {Rousseau, Christina},
Title = {{Wages Due Lesbians: Visibility and Feminist Organizing in 1970s Canada}},
Journal = {{GENDER WORK AND ORGANIZATION}},
Year = {{2015}},
Volume = {{22}},
Number = {{4}},
Pages = {{364-374}},
Month = {{JUL}},
Abstract = {{In this article I look at the emergence of Wages Due Lesbians, a lesbian
   group that was part of the Marxist feminist group Wages for Housework in
   Canada. In presenting Wages Due as a historical case study, I re-visit
   the notion of visibility' in relation to lesbian motherhood in Canada in
   the 1970s through an examination of struggles for welfare, child
   custody, and against violence. Through this case study I present the
   shifting ideas regarding respectability and homosexuality from the 1970s
   to today.}},
