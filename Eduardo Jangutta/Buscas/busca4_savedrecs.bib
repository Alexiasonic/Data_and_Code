@article{ ISI:000426243300005,
Author = {Conaghan, Joanne},
Title = {{Labour Law and Feminist Method}},
Journal = {{INTERNATIONAL JOURNAL OF COMPARATIVE LABOUR LAW AND INDUSTRIAL RELATIONS}},
Year = {{2017}},
Volume = {{33}},
Number = {{1}},
Pages = {{93-114}},
Abstract = {{This article explores the application of feminist method in the context
   of contemporary scholarly efforts to reclaim and/or refashion labour law
   as a discipline and field of study. The central methodological
   importance of gender as a category of analysis is highlighted and common
   critical techniques deployed by feminists to advance gender-inflected
   analysis identified and illustrated. A core insight the article seeks to
   advance is that because mainstream labour law scholars tend to approach
   feminism as animated solely by gender equality concerns, they overlook
   the broader analytical and conceptual contribution that feminist
   scholars can and do make to tackling and resolving key challenges and
   concerns arising from the social organization of work and its
   regulation.}},
