@article{ ISI:000454512800003,
Author = {McGuire, John Thomas},
Title = {{Between Two Eras: Anna Rosenberg and the Maintaining of the Alternative
   View of Public Administration, 1941-1945}},
Journal = {{ADMINISTRATION \& SOCIETY}},
Year = {{2019}},
Volume = {{51}},
Number = {{2}},
Pages = {{229-249}},
Month = {{FEB}},
Abstract = {{Since the early 1990s, scholars have expanded their examination of
   historical context in the development of public administration in the
   United States, particularly in the area of gender. No one, however, has
   examined how the alternative, or social-justice-centered, view of public
   administration survived between its initial period of development and
   its revival after World War II. This article fills that gap by
   demonstrating how Anna Lederer Rosenberg, one of President Franklin D.
   Roosevelt's key advisors from 1941 through 1945, maintained the
   alternative view through her political facilitation and national
   visibility.}},
