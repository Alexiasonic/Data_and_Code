@article{ ISI:000472976400001,
Author = {Redfern, Neil},
Title = {{Women hold up half the sky: The response of Maoists in Britain to the
   challenge of feminism}},
Journal = {{JOURNAL OF LABOR AND SOCIETY}},
Year = {{2019}},
Volume = {{22}},
Number = {{2, SI}},
Pages = {{243-257}},
Month = {{JUN}},
Abstract = {{In recognition of their vital contribution to human society, Mao
   remarked that ``Women Hold Up Half the Sky.{''} But vital or not women
   had been oppressed since the emergence of class society. The failure of
   the left, including the revolutionary left, to take seriously and to
   combat women's oppression was a major factor in the emergence of the new
   women's movement out of the ideological and political ferment of ``The
   Sixties{''} (ca. 1966-1979). Here, we will concentrate on the two most
   significant Maoist organizations, the Communist Federation of Britain
   (Marxist-Leninist) (which united with another Maoist organization to
   become the Revolutionary Communist League of Britain in 1977) and the
   Communist Party of Britain (Marxist-Leninist). Second wave feminism
   posed a serious challenge to the class analysis favored by the left.
   Marxists accepted that women were oppressed in class society. But was
   the oppression of women primarily a phenomena of class society or was
   it, as many feminists argued, primarily a manifestation of patriarchy?
   The oppression of women is rarely, if ever, seen as the most or even as
   a pressing question by Marxist revolutionary organizations. One reason
   for this of course is that such organizations are usually numerically
   dominated by men.}},
