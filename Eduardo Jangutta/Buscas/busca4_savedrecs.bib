@article{ ISI:000246693800001,
Author = {Harney, Stefano},
Title = {{Socialization and the business school}},
Journal = {{MANAGEMENT LEARNING}},
Year = {{2007}},
Volume = {{38}},
Number = {{2}},
Pages = {{139-153}},
Month = {{APR}},
Abstract = {{There is a new phase in the generalization of management capacities, but
   contrary to the assumptions of critical management educators, the
   investment in the business school has not been to socialize more
   students into this generalized management, but to seek the principle of
   generalization in these students themselves as part of a struggle
   between capital and labour. Using the insights of autonomist feminist
   theorists, this article attempts to analyse why critical management
   education has been unable to find a new object appropriate to this new
   generalization of management, and speculates on what the critical and
   political benefits might be of escaping older notions of the business
   school as a site of socialization for a social category of managers.}},
