@article{ ISI:000432678100004,
Author = {Naude, Piet},
Title = {{Can we overcome the anthropoceritrism bias in sustainability discourse?}},
Journal = {{AFRICAN JOURNAL OF BUSINESS ETHICS}},
Year = {{2018}},
Volume = {{11}},
Number = {{2}},
Pages = {{56-67}},
Month = {{FEB}},
Abstract = {{Based on a turn to the rational human subject in Descartes, Kant and
   Feuerbach, this article critically examines four efforts at shaping
   sustainability discourse: the definition of sustainability in our common
   future; stewardship in Christian theology; forms of partisan justice;
   and GDP as measure of economic growth. These efforts made certain
   advances, but because they share the underlying anthropocentric bias of
   Western philosophy, they fail to step out of the current sustainability
   paradigm. The article closes with two suggestions of how to decentre the
   human subject and build a network-view of all species.}},
