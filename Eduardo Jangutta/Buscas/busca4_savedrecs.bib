@article{ ISI:000322161100008,
Author = {Parsons, Elizabeth and Priola, Vincenza},
Title = {{Agents for Change and Changed Agents: The Micro-politics of Change and
   Feminism in the Academy}},
Journal = {{GENDER WORK AND ORGANIZATION}},
