@article{ ISI:000515400300001,
Author = {Kurtege Sefer, Bengu},
Title = {{A gender- and class-sensitive explanatory model for rural women
   entrepreneurship in Turkey}},
Journal = {{INTERNATIONAL JOURNAL OF GENDER AND ENTREPRENEURSHIP}},
