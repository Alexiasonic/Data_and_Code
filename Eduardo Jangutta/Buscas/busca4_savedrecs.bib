@article{ ISI:000566396300001,
Author = {Lewellen, Chelesea and Bohonos, Jeremy W.},
Title = {{Excuse me, sir?: A critical race theory (hair) chronicle}},
Journal = {{GENDER WORK AND ORGANIZATION}},
Abstract = {{While engaged in research and teaching regarding racism in US
   organizations, members of the writing team have frequently found that
   conscientious humour and the meaningful lessons it can encode provide
   needed releases from the stress associated with social justice work as
   well as serve as an effective pedagogical tool. Taking inspiration from
   contemporary satirical humourists, including Robin Thede, Issa Rae, Dave
   Chappelle and Jordan Peele, who have used comedy to expose serious
   aspects of racism, as well as leading critical race theorists' use of
   fictional chronicles about race we present a comedic chronicle centred
   around an African American protagonist who demonstrates a variety of
   coping and resistance strategies in conversations with a
   well-intentioned yet culturally insensitive white male co-worker who
   regularly engages her about her hair.}},
